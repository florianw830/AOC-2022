    "def isVisible(l,x,y):\n",
    "    skip = []\n",
    "    ret = [0,0,0,0]\n",
    "    tmp = 0\n",
    "    for _y in range(y+1,len(ll)):\n",
    "        tmp+=1\n",
    "        if l[_y][x] >= l[y][x]:          \n",
    "            break\n",
    "    ret[0] = tmp\n",
    "    tmp = 0\n",
    "    for _y in range(y-1,-1,-1):\n",
    "        tmp+=1\n",
    "        if l[_y][x] >= l[y][x]:\n",
    "            break\n",
    "    ret[1] = tmp\n",
    "    tmp = 0\n",
    "    for _x in range(x+1,len(ll[0])):\n",
    "        tmp+=1\n",
    "        if l[y][_x] >= l[y][x]:\n",
    "            break\n",
    "    ret[2] = tmp\n",
    "    tmp = 0\n",
    "    for _x in range(x-1,-1,-1):\n",
    "        tmp+=1\n",
    "        if l[y][_x] >= l[y][x]:\n",
    "            break\n",
    "    ret[3] = tmp\n",
    "    #print(x,y,ret)\n",
    "    return ret[0] *ret[1] * ret[2] * ret[3]\n",
    "    \n"
   ]
